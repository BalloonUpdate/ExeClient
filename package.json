{
  "name": "updater-for-minecraft",
  "version": "0.0.0",
  "author": {
    "name": "innc11",
    "email": "asforest@qq.com"
  },
  "main": "compiled/main/main.js",
  "scripts": {
    "dev": "chcp 65001 && electron --trace-warnings ./compiled/main/main.js",
    "dev_tsc": "tsc -w",
    "dev_build": "yarn run build_electron && yarn run _pack -o debug-directory/um.exe -x",
    "dev_build_console": "yarn run build_electron && yarn run _pack -o debug-directory/um.exe",
    "compile": "tsc",
    "electron": "yarn run build_compile && electron-builder --dir",
    "_pack": "bootloader/lw.exe -p dist/win-unpacked -c updater-for-minecraft.exe"
  },
  "devDependencies": {
    "electron": "^13.1.4",
    "electron-builder": "^22.11.7",
    "typescript": "^4.3.4"
  },
  "dependencies": {
    "crypto": "^1.0.1",
    "iconv-lite": "^0.6.3",
    "js-yaml": "^4.1.0",
    "moment": "^2.29.1",
    "node-fetch": "^2.6.1"
  }
}
